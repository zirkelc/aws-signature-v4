{
  "$schema": "https://biomejs.dev/schemas/1.5.1/schema.json",
  "vcs": {
    "clientKind": "git",
    "enabled": true,
    "useIgnoreFile": true
  },
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "suspicious": {
        "noExplicitAny": "off"
      },
      "complexity": {
        "useLiteralKeys": "off",
        "noForEach": "off"
      }
    }
  }
}